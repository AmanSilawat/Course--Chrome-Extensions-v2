{
	"manifest_version": 2,
	"name": "Twitter Social Toolkit",
	"description": "Twitter Social Toolkit",
	"version": "1.0",
	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default": "icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "icon.png",
		"64": "icon.png",
		"32": "icon.png",
		"16": "icon.png"
	},
	"permissions": ["contextMenus"]
}
